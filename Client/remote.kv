#:kivy 1.0

<Remote>:
    status_line: status

    BoxLayout:
        orientation: 'vertical'
        padding: 5

        Button:
            text: 'PLAY'
            on_press: root.playButton()
        Button:
            text: 'QUIT'
            on_press: root.quitButton()
        Label:
            id: status
            text: ''
